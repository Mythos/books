{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "enlightn/enlightn": "2.10.0",
        "guzzlehttp/guzzle": "7.8.1",
        "intervention/image": "3.5.1",
        "jantinnerezo/livewire-alert": "2.2.7",
        "laravel/framework": "10.48.7",
        "laravel/sanctum": "3.3.3",
        "laravel/tinker": "2.9.0",
        "laravel/ui": "4.5.1",
        "livewire/livewire": "2.12.6",
        "nicebooks/isbn": "0.4.21",
        "php-flasher/flasher": "1.13.1",
        "php-flasher/flasher-toastr": "1.13.1",
        "php-flasher/flasher-laravel": "1.13.1",
        "php-flasher/flasher-toastr-laravel": "1.13.1",
        "spatie/laravel-sluggable": "3.6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.13.4",
        "barryvdh/laravel-ide-helper": "3.0.0",
        "fakerphp/faker": "1.23.1",
        "mockery/mockery": "1.6.11",
        "nunomaduro/collision": "7.10.0",
        "phpunit/phpunit": "11.4.2",
        "spatie/laravel-ignition": "2.5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}